<div id="contentBk" class="clearfix">
            <div id="content">
                <div class="topImg clearfix">
                    <%= image_tag("headers/header_1.jpg", :alt => "Course Listings") %>
                    <p>Course <strong>Listings</strong></p>
                </div>
                <div class="wrapper">
                    <p id="notice"><%= notice %></p>

                        <h1>Levels</h1>

                        <table>
                          <thead>
                            <tr>
                              <th>Leveltype</th>
                              <th colspan="3"></th>
                            </tr>
                          </thead>

                          <tbody>
                            <% @levels.each do |level| %>
                              <tr>
                                <td><%= level.leveltype %></td>
                                <td><%= link_to 'Show', level %></td>
                                <td><%= link_to 'Edit', edit_level_path(level) %></td>
                                <td><%= link_to 'Destroy', level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                              </tr>
                            <% end %>
                          </tbody>
                        </table>

                        <br>

                        <%= link_to 'New Level', new_level_path %>

                    <div class="clear"></div>

                    
                </div>


            </div>
        </div>















